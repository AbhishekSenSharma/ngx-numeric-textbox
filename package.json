{
  "name": "ngx-numeric-textbox",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "start": "ng serve",
    "lint": "ng lint",
    "test": "ng test",
    "test-coverage": "ng test --code-coverage",
    "test-ci": "ng test --watch=false --code-coverage --single-run=true",
    "e2e": "ng e2e",
    "ci": "npm run lint && npm run test-ci && npm run e2e",
    "prebuild:lib": "rimraf tmp dist/lib",
    "build:lib": "gulp inline-templates && ngc -p tsconfig-lib.json && copyfiles package.json README.md LICENSE dist/lib",
    "prebuild:demo": "rimraf dist/demo",
    "build:demo": "ng build --prod",
    "pub:lib": "npm run build:lib && npm publish dist/lib",
    "pub:demo": "npm run build:demo -- --base-href /ngx-numeric-textbox/ && gh-pages -d dist/demo",
    "pub": "npm run pub:lib && npm run pub:demo"
  },
  "keywords": [
    "ng",
    "ngx",
    "angular",
    "numeric",
    "textbox",
    "ngx-numeric-textbox",
    "angular numeric textbox component"
  ],
  "author": "Leo Vo <leovo2708@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leovo2708/ngx-numeric-textbox.git"
  },
  "bugs": {
    "url": "https://github.com/leovo2708/ngx-numeric-textbox/issues"
  },
  "main": "./bundles/ngx-treeview.umd.js",
  "module": "./src/index.js",
  "jsnext:main": "./src/index.js",
  "typings": "./src/index.d.ts",
  "private": false,
  "peerDependencies": {
    "@angular/common": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "lodash": "^4.0.0",
    "numeral": "^2.0.0"
  },
  "devDependencies": {
    "@angular/animations": "^4.0.0",
    "@angular/cli": "1.2.5",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/compiler-cli": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/language-service": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "bootstrap": "^4.0.0-alpha.6",
    "codelyzer": "~3.0.1",
    "copyfiles": "^1.2.0",
    "core-js": "^2.4.1",
    "gh-pages": "^1.0.0",
    "gulp": "^3.9.1",
    "gulp-inline-ng2-template": "^4.0.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lodash": "^4.17.4",
    "node-sass": "^4.5.3",
    "numeral": "^2.0.6",
    "protractor": "~5.1.2",
    "rimraf": "^2.6.1",
    "rxjs": "^5.4.1",
    "ts-node": "~3.0.4",
    "tslint": "~5.3.2",
    "typescript": "~2.3.3",
    "zone.js": "^0.8.14"
  }
}
